  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start SEQUENTIAL Server",
      "type": "debugpy",
      "request": "launch",
      "program": "cli.py",
      "console": "integratedTerminal",
        "args": ["serve", "--queue-mode", "sequential"]
    },
        {
      "name": "Start BATCHED Server",
      "type": "debugpy",
      "request": "launch",
      "program": "cli.py",
      "console": "integratedTerminal",
        "args": ["serve", "--queue-mode", "batched"]
    },
    {
      "name": "Predict",
      "type": "debugpy",
      "request": "launch",
      "program": "cli.py",
      "console": "integratedTerminal",
        "args": ["predict", "--text", "The future of AI is", "--top-k", "25"]
    },
    {
      "name": "Generate",
      "type": "debugpy",
      "request": "launch",
      "program": "cli.py",
      "console": "integratedTerminal",
        "args": ["generate", "--text", "The future of AI is"]
    },
    {
      "name": "Benckmark",
      "type": "debugpy",
      "request": "launch",
      "program": "cli.py",
      "console": "integratedTerminal",
        "args": ["benchmark", "--text", "The future of AI is"]
    }
  ]
}
